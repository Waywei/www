doctype 5
html
  head
    meta(charset='utf-8')
    meta(name="viewport",content="width=device-width, initial-scale=1.0, maximum-scale=1,user-scalable=no")
    block title
    if config.DEV
      link(href="{{config.SURL}}static/fontawesome/css/font-awesome.min.css",rel="stylesheet")
      script(src='{{config.SURL}}static/js/jquery.js')
    else
      link(href="http://libs.baidu.com/fontawesome/4.0.3/css/font-awesome.min.css",rel="stylesheet")
      script(src='http://lib.sinaapp.com/js/jquery/1.8.3/jquery.min.js')

    link(rel='stylesheet',href='{{config.SURL}}static/css/app.css')
    script(src='{{config.SURL}}static/js/app.js')
    block head
  body
    #wrap
      include base/topnav
      block main
      include base/footer

